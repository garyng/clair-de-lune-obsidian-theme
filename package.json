{
  "name": "clair-de-lune-obsidian-theme",
  "version": "1.4.1",
  "description": "An Obsidian.md theme based on the Moonlight theme family.",
  "main": "index.js",
  "scripts": {
    "build": "sass src/index.scss ./dist/obsidian.css",
    "watch": "npm-watch build",
    "lint:fix": "prettier -w ./src",
    "lint:check": "prettier -c ./src"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "scss"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "npm-watch": "^0.7.0",
    "sass": "^1.26.10"
  },
  "devDependencies": {
    "prettier": "^2.1.2"
  }
}
